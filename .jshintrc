{
  "node": true,
  "browser": true,
  "jasmine": true,
  "esnext": true,
  "bitwise": false,
  "camelcase": false,
  "curly": false,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "newcap": true,
  "noarg": true,
  "quotmark": "single",
  "regexp": true,
  "undef": true,
  "unused": true,
  "strict": false,
  "trailing": true,
  "smarttabs": false,
  "expr": true,
  "globals": {
    "angular": true
  },
  "predef": [
    "inject",
    "StringMask",
    "BrV"
  ],
  "indent": 2,
  "maxlen": 120,
  "devel": false,
  "noempty": true
}
